 select * from work_table;

 select name,class,addr,javaScore
 from work_table;

 update  work_table
 set gender='남자',javaScore=90, class='A', tel='010-2234-7611'
 where num=3;

 select * from work_table;

 update  work_table
 set  class='B', tel='010-1111-2341'
 where name='임태균';

  select * from work_table;

 commit;

 delete from work_table
 where class='A';

 select * from work_table;

 rollback;

 select * from work_table;


 drop table work_table;
 show recyclebin;
 flashback table work_table
 to before drop;

 truncate table work_table;
 drop table work_table;

 show recyclebin;
 purge recyclebin;
