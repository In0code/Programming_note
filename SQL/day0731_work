create table work_table(
num number(5),
name varchar2(30),
class char(1),
email varchar2(50),
tel varchar2(16),
addr varchar2 (240),
gender char (6),
javascore number(3),
input_date date
);



insert into work_table
 values(1,'임태균' ,'A','tae@sist.co.kr','010-1234-5678','경기도 용인시','남자',91,sysdate);
 commit;

