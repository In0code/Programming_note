package day0821_work;

public class EmpInfoVO {

	private String ename, hiredate, job;
	private int empno, sal;

	public String getEname() {
		return ename;
	}

	public void setEname(String ename) {
		this.ename = ename;
	}

	public String getHiredate() {
		return hiredate;
	}

	public void setHiredate(String hiredate) {
		this.hiredate = hiredate;
	}

	public String getJob() {
		return job;
	}

	public void setJob(String job) {
		this.job = job;
	}

	public int getEmpno() {
		return empno;
	}

	public void setEmpno(int empno) {
		this.empno = empno;
	}

	public int getSal() {
		return sal;
	}

	public void setSal(int sal) {
		this.sal = sal;
	}

	@Override
	public String toString() {
		return "\n사원번호 : " + empno + "\n사원명 : " + ename + "\n입사일 : " + hiredate + "\n직무 : " + job +   "\n 연봉 : "
				+ sal ;
	}

	public EmpInfoVO( int empno,String ename, String hiredate, String job, int sal) {
		super();
		this.ename = ename;
		this.hiredate = hiredate;
		this.job = job;
		this.empno = empno;
		this.sal = sal;
	}

	public EmpInfoVO() {
		super();
	}

}
